{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ui",
      "image": "733997234712.dkr.ecr.us-east-1.amazonaws.com/app-ui:latest",
      "hostname": "ui",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 4200,
          "containerPort": 4200
        }
      ]
    },
    {
      "name": "api",
       "image": "733997234712.dkr.ecr.us-east-1.amazonaws.com/app-api:latest",
      "hostname": "api",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "733997234712.dkr.ecr.us-east-1.amazonaws.com/app-nginx:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["ui", "api"],
      "memory": 512
    }
  ]
}
